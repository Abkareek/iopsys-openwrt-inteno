
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option mtu '1500'
	option ifname 'eth6 eth5'
	option ip6assign '60'

config interface 'wan'
	option type 'anywan'
	option proto 'dhcp'
	option mtu '1500'
	option ifname 'eth2.1  '
	option ipv6 '1'
	option hostname 'OWNIT_EG300'
	option vendorid 'OWNIT_EG300'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'IPTV'
	option type 'bridge'
	option proto 'none'
	option ifname 'eth2.501 eth3 eth4'
	option auto '1'
