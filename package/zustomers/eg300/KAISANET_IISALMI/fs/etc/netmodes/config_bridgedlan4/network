
config interface 'loopback'
        option is_lan '1'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'lan'
        option is_lan '1'
        option type 'bridge'
        option proto 'static'
        option ipaddr '192.168.1.1'
        option netmask '255.255.255.0'
        option mtu '1500'
        option ip6assign '60'
        option ifname 'eth6 eth5 eth3'

config interface 'wan'
        option proto 'dhcp'
        option hostname 'Inteno_0775'
        option vendorid 'EG300A-WU21UAC-INTENO'
        option mtu '1500'
        option ipv6 '1'
        option type 'bridge'
        option ifname 'eth2.1 eth4'

config interface 'wan6'
        option proto 'dhcpv6'
        option ifname '@wan'
