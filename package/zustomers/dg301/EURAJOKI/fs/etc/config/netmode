config mode 'setup'
        option dir '/etc/netmodes'
        option detail ''
        option curmode 'default'

config netmode 'default'
        option desc 'Default'
        option conf 'config_bridged'
        option exp 'This mode uses firmware default settings'

config netmode 'routed'
        option desc 'Fully Routed (NAT)'
        option conf 'config_routed'
        option exp 'All LAN ports fully routed (NAT)'

