
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option ifname 'eth3 eth4'

config interface 'wan'
	option type 'anywan'
	option ifname 'ptm0.1 atm0.1 eth0.1'
	option vendorid 'DG301A-W7P2UD-INTENO'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'iptv'
	option type 'bridge'
	option proto 'none'
	option ifname 'atm0.63 eth0.63 eth1 eth2 ptm0.63'
	option auto '1'

