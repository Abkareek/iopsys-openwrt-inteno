
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ifname 'eth1 eth2 eth3 eth4'
	option ip6assign '60'
	option bridge_instance '1'

config interface 'wan'
	option type 'anywan'
	option vendorid '$HARDWAREID-INTENO'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'
	option ifname 'atm0.1 eth0.1 ptm0.1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'wan2'
        option proto 'dhcp'
        option ifname 'atm1.1'
        option vendorid '$HARDWAREID-INTENO'
        option hostname 'Inteno_$MAC4'
        option ipv6 '1'

config interface 'wan2v6'
        option proto 'dhcpv6'
        option ifname '@wan2'


