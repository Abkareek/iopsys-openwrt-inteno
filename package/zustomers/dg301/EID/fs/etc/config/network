
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option ipaddr '192.168.2.1'
	option bridge_instance '1'
	option ifname 'eth1 eth3 eth4'

config interface 'wan1'
	option type 'anywan'
	option proto 'dhcp'
	option ifname 'ptm0.1 atm0.1 eth0.1'
	option vendorid 'DG301A-W7P2UD-INTENO'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'
	option wan_instance '1.1.1.1'

config interface 'wan1v6'
	option proto 'dhcpv6'
	option ifname '@wan1'
	option reqaddress 'try'
	option reqprefix '56'

config interface 'wan2'
	option proto 'dhcp'
	option ifname 'atm1.1'
	option vendorid 'DG301A-W7P2UD-INTENO'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'
	option wan_instance '2.2.1.1'

config interface 'wan2v6'
	option proto 'dhcpv6'
	option ifname '@wan2'

config interface 'WLAN_GUEST'
	option is_lan '1'
	option proto 'static'
	option netmask '255.255.255.0'
	option ipaddr '192.168.3.1'
	option ip6assign '64'

config available-bridge
	option baseifname 'eth0'
	option key '1'

config available-bridge
	option baseifname 'atm0'
	option key '2'

config available-bridge
	option baseifname 'atm1'
	option key '3'

config available-bridge
	option baseifname 'ptm0'
	option key '4'

config available-bridge
	option baseifname 'eth1'
	option key '5'

config available-bridge
	option baseifname 'eth2'
	option key '6'

config available-bridge
	option baseifname 'eth3'
	option key '7'

config available-bridge
	option baseifname 'eth4'
	option key '8'

config interface 'IPTV'
	option type 'bridge'
	option proto 'none'
	option ifname 'eth0.10 eth2'
	option bridge_instance '2'

config marking-bridge
	option baseifname 'eth0'
	option bridgekey '2'
	option interfacekey '1'
	option instance '1'

