config general 'polling'
	option enabled 'on'
	option starttime '03'
	option interval 'daily'

config server 'configserver'
        option enabled 'off'
	option reboot 'on'

config server 'iup'
	option enabled 'on'

config software 'uppgradeserver'
	option enabled 'on'
	option defaultreset 'off'
	option url 'http://www.loqal.no/ftth/DG301-W7P2U_INT2.4.11RC1-150116_1829.w'
