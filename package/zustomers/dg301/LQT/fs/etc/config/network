config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ifname 'eth4 eth3 eth1 eth2'
	option ip6assign '60'

config interface 'wan'
	option type 'anywan'
	option ifname 'ptm0.1 atm0.1 eth0.1'
	option vendorid 'DG301B-W7P2UD-INTENO'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'

config interface 'wan2'
	option type 'anywan'
	option ifname 'atm1.1'
	option vendorid 'DG301B-W7P2UD-INTENO'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option ipv6 '1'

config globals 'globals'
	option ula_prefix 'fdc4:01a1:7251::/48'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'Mobilt'
        option proto '3g'
        option device '/dev/ttyUSB0'
        option service 'umts'
        option apn 'internet.public'
        option pincode '0000'
