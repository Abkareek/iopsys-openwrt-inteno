config easybox-settings settings
	option usb_port '1'
	option status_led '1'
	option power_led '1'
	option power_led_br '100'
	option wifibutton '1'
	option wpsbutton '1'
	option wpsdevicepin '1'

config easybox-services network
	option internet 'GPON_Internet'
	option voice 'GPON_VoIP'
	option iptv 'GPON_IPTV'
