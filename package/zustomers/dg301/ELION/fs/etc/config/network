
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ifname 'eth1 eth2 eth3 eth4'
	option ip6assign '64'
	option ip6hint '1'

config interface 'Servicelan'
        option is_lan '1'
        option proto 'static'
        option ifname '@lan'
        option ipaddr '172.16.30.1'
        option netmask '255.255.255.0'

config interface 'guest'
	option is_lan '1'
	option proto 'static'
	option ipaddr '192.168.10.1'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option ip6hint '2'

config interface 'NutiTV'
	option type 'anywan'
	option proto 'dhcp'
	option gateway '0.0.0.0'
	option reqopts 'staticroutes'
	option ifname 'ptm0.4 atm1.1 eth0.4'
	#option dns '0.0.0.0'
	#option peerdns '0'
	option defaultroute '0'
	
config interface 'internet'
	option type 'anywan'
	option proto 'dhcp'
	option vendorid 'DG301-W7P2U-INTENO'
	option hostname 'Inteno_$MAC4'
	option ifname 'ptm0.1 atm0.1 eth0.1'

config interface 'internet6'
	option proto 'dhcpv6'
	option ifname '@internet'
	option reqaddress 'none'
	option reqprefix 'auto'

config interface 'mobile'
	option proto '3g'
	option apn 'internet.emt.ee'
	option service 'umts'
	option pincode '0000'
	option device '/dev/ttyUSB0'

