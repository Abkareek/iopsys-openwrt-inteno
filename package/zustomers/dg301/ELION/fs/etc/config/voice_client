config sip_service_provider 'sip0'
        option name 'Account 1'
        option enabled '0'
        option codec0 'alaw'
        option codec1 'ulaw'
        option codec2 'g729'
        option cfim_on '*21*'
        option cfim_off '#21#'
        option cfbs_on '*61*'
        option cfbs_off '#61#'
        option call_return '*69'
        option redial '*66'
        option cbbs_key '5'
        option cbbs_maxretry '5'
        option cbbs_retrytime '300'
        option cbbs_waittime '30'
        option is_fax '0'
	option host 'elion.ee'
        list direct_dial '_[3467]XXXXXX'
        list direct_dial '_[47]0XXXXXX'
        list direct_dial '_372[3467]XXXXXX'
        list direct_dial '_372[47]0XXXXXX'
        list direct_dial '_3725X.'
        option call_lines  '4'
        option outboundproxy '0.0.0.0'

config sip_service_provider 'sip1'
        option name 'Account 2'
	option enabled '0'
        option codec0 'alaw'
        option codec1 'ulaw'
        option codec2 'g729'
        option cfim_on '*21*'
        option cfim_off '#21#'
        option cfbs_on '*61*'
        option cfbs_off '#61#'
        option call_return '*69'
        option redial '*66'
        option cbbs_key '5'
        option cbbs_maxretry '5'
        option cbbs_retrytime '300'
        option cbbs_waittime '30'
        option is_fax '0'
	option host 'elion.ee'
        list direct_dial '_[3467]XXXXXX'
        list direct_dial '_[47]0XXXXXX'
        list direct_dial '_372[3467]XXXXXX'
        list direct_dial '_372[47]0XXXXXX'
        list direct_dial '_3725X.'
        option call_lines '5'
	option outboundproxy '0.0.0.0'

config brcm_line 'brcm0'
        option extension '0000'
        option sip_account 'sip0'
	option echo_cancel '1'
        option ptime '20'
        option name 'DECT 1'

