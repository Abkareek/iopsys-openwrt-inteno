config cwmp 'acs'
        option userid '$OUI-$SER'
        option passwd 'inteno'
        option periodic_inform_interval '86400'
        option periodic_inform_time '0'
        option dhcp_url_path 'provisioning.iup.url'
        option periodic_inform_enable '0'
        option dhcp_discovery 'disable'
        option url 'acsi.kaisanet.fi'

config cwmp 'cpe'
        option default_wan_interface 'wan'
        option manufacturer 'Inteno'
        option log_to_console 'disable'
        option log_to_file 'enable'
        option log_severity 'INFO'
        option log_file_name '/var/log/cwmpd.log'
        option log_max_size '102400'
        option userid '$OUI-$SER'
        option passwd 'cpeinteno'
        option port '7547'
        option ubus_socket '/var/run/ubus.sock'
        option interface 'br-wan'

config cwmp
        option parameter 'InternetGatewayDevice.DeviceInfo.SpecVersion'
        option value '1.0'

