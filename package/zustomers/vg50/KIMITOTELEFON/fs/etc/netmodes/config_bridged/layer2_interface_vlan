	option ifname 'ptm0.2708'
	option baseifname 'ptm0'
	option vlan8021q '2708'
	option vlan8021p '0'
	option bridge '1'

config vlan_bridge
	option instance '2.1'
	option bridgekey '2'
	option vid '713'
	option name 'vlan_2.1'

config vlan_bridge
	option instance '3.1'
	option bridgekey '3'
	option vid '863'
	option name 'vlan_3.1'

