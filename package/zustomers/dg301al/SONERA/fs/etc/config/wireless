
config wifi-device 'wl0'
	option type 'broadcom'
	option country 'EU/13'
	option band 'a'
	option bandwidth '40'
	option hwmode '11ac'
	option channel 'auto'
	option scantimer '15'
	option wmm '1'
	option wmm_noack '0'
	option wmm_apsd '0'
	option txpower '100'
	option rateset 'default'
	option frag '2346'
	option rts '2347'
	option dtim_period '1'
	option beacon_int '100'
	option rxchainps '0'
	option rxchainps_qt '10'
	option rxchainps_pps '10'
	option rifs '0'
	option rifs_advert '0'
	option doth '0'
	option maxassoc '16'

config wifi-iface
	option device 'wl0'
	option network 'lan'
	option mode 'ap'
	option ssid 'SoneraGateway-002207$BSSID6'
	option encryption 'mixed-psk'
	option key '$WPAKEY'
	option gtk_rekey '3600'
	option macfilter '0'
	option wmf_bss_enable '1'
	option bss_max '16'
	option cipher 'auto'

config wifi-device 'wl1'
	option type 'broadcom'
	option country 'EU/13'
	option band 'b'
	option bandwidth '20'
	option hwmode 'auto'
	option channel 'auto'
	option scantimer '15'
	option wmm '1'
	option wmm_noack '0'
	option wmm_apsd '0'
	option txpower '100'
	option rateset 'default'
	option frag '2346'
	option rts '2347'
	option dtim_period '1'
	option beacon_int '100'
	option rxchainps '0'
	option rxchainps_qt '10'
	option rxchainps_pps '10'
	option rifs '0'
	option rifs_advert '0'
	option doth '0'
	option maxassoc '16'
	option txantenna '3'
	option rxantenna '3'

config wifi-iface
	option device 'wl1'
	option network 'lan'
	option mode 'ap'
	option encryption 'mixed-psk'
	option key '$WPAKEY'
	option gtk_rekey '3600'
	option macfilter '0'
	option wmf_bss_enable '1'
	option ssid 'SoneraGateway-002207$BSSID6'
	option bss_max '16'
	option cipher 'auto'

config 'wifi-iface'
        option 'device' 'wl1'
        option 'network' 'guest'
        option 'mode' 'ap'
        option 'encryption' 'none'
        option 'macfilter' '0'
        option 'ssid' 'Guest-SoneraGateway'
        option 'wps_pbc' '0'
        option 'wmf_bss_enable' '1'
        option 'isolate' '1'
        option 'disabled' '1'
