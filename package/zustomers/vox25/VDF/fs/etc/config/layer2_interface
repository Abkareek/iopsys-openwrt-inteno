
config dsltype  'vdsl'
        option 'enabled' 'on'

config dsltype  'adsl'
        option 'enabled' 'on'

config dslsettings 'capabilities'
	option GDmt 'Enabled'
	option ADSL2 'Enabled'
	option ADSL2plus 'Enabled'
	option VDSL2 'Enabled'
	option 12b 'Enabled'
	option 17a 'Enabled'
	option 30a 'Enabled'
	option US0 'on'
	option bitswap 'on'
	option sra 'on'






