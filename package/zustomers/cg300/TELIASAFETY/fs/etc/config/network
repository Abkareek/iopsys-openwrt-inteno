config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ifname 'eth1'
	option ip6assign '60'

config interface 'wan'
	option proto 'dhcp'
	option hostname 'Inteno_E8B8'
	option vendorid 'CG300A-WU21UD-INTENO'
	option mtu '1500'
	option ifname 'eth0.1'
	option ipv6 '1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'mobile'
	option proto '4g'
	option modem 'ncm:ttyUSB1:wwan0'
	option service 'ncm'
	option comdev '/dev/ttyUSB1'
	option ifname 'wwan0'
	option apn 'internet.ts.m2m'
	option pincode '0000'

