
config snmp 'daemon'
	option enabled '1'

config agent
	option agentaddress 'UDP:161'

config com2sec 'public'
	option secname 'ro'
	option source 'default'
	option community 'Waf99or55'

config group 'public_v2c'
	option version 'v2c'
	option secname 'ro'
	option group 'Waf99or55'

config view 'all'
	option viewname 'all'
	option type 'included'
	option oid '.1'

config access 'public_access'
	option context 'none'
	option version 'any'
	option level 'noauth'
	option prefix 'exact'
	option read 'all'
	option write 'none'
	option notify 'none'
	option group 'Waf99or55'

config system
	option sysContact 'teknisk@nordix.no'
	option sysName 'Nordix Data AS'
	option sysLocation 'Nordixnett'

config exec
	option name 'filedescriptors'
	option prog '/bin/cat'
	option args '/proc/sys/fs/file-nr'

