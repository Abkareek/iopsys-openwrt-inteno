
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option ifname 'eth6 eth5'
	option ip6assign '60'

config interface 'wan'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option vendorid 'EG300A-WU21U-INTENO'
	option mtu '1500'
	option ipv6 '1'
	option type 'anywan'
	option ifname 'eth2.888'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'mgmt'
	option proto 'dhcp'
	option ifname 'eth2.66'
	option defaultroute '0'
	
config interface 'iptv'
	option type 'bridge'
	option ifname 'eth2.1511 eth3 eth4'
	option proto 'none'
	option auto '1'
	

