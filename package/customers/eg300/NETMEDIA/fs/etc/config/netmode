config mode 'setup'
        option dir '/etc/netmodes'
        option detail 'To change network configuration to preconfigured please select configuration mode from dropdown menu and press button'
        option curmode 'routed'

config netmode 'routed'
        option desc 'Routed'
        option conf 'config_routed'
        option exp 'Routed changes your modem to use NAT'

config netmode 'bridged'
        option desc 'Bridged'
        option conf 'config_bridged'
        option exp 'Bridged mode changes your modem to bridged'
