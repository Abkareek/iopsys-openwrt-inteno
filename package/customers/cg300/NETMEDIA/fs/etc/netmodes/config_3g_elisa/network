
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option mtu '1500'
	option ifname 'eth1'
	option ip6assign '60'

config interface 'wan'
	option type 'anywan'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option vendorid 'CG300A-WU21UD-INTENO'
	option mtu '1500'
	option ifname 'eth0.1  '
	option ipv6 '1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'

config interface 'wan2'
	option proto '3g'
	option device '/dev/ttyUSB0'
	option service 'umts'
	option apn 'internet'
	option pincode '1234'

