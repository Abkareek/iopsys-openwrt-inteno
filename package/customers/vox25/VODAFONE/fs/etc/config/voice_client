	option cbbs_maxretry '5'
	option cbbs_retrytime '300'
	option cbbs_waittime '30'
	option domain 'ims.vodafone.pt'
	option codec0 'alaw'
	option codec1 'g729'
	option port '5060'
	option outboundproxy 'stpa.ims.vodafone.pt'
	option outboundproxyport '5060'
	option is_fax '0'
	option enabled '1'
	option host 'ims.vodafone.pt'
	option priority_ulaw '1'
	option priority_alaw '1'
	option priority_g729 '2'
	option priority_g723 '1'
