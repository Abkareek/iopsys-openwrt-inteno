
config brcm_line 'brcm0'
	option extension '0000'
	option ptime '20'
	option noise '0'
	option vad '0'
	option txgain '0'
	option rxgain '0'
	option name 'TEL 1'
	option instances '1'
	option sip_account 'sip0'

config brcm_line 'brcm1'
	option extension '1111'
	option ptime '20'
	option noise '0'
	option vad '0'
	option txgain '0'
	option rxgain '0'
	option name 'TEL 2'
	option instances '2'
	option sip_account 'sip1'

config dialplan 'custom_dialplan'
	option custom_outgoing_enabled '0'
	option custom_incoming_enabled '0'
	option custom_hangup_enabled '0'
	option all_ports_extension '#123456'
	option test_audio_extension '#123457'
	option test_echo_extension '#123458'

config sip_advanced 'SIP'
	option rtpstart '10000'
	option rtpend '20000'
	option dtmfmode 'rfc2833'
	option remotehold 'yes'
	option bindintf 'GPON_VoIP'
	option bindport '5060'
	option sip_proxy 'stpa.ims.vodafone.pt'

config brcm_advanced 'BRCM'
	option jbenable 'yes'
	option jbforce 'no'
	option jbmaxsize '500'
	option jbimpl 'adaptive'
	option genericplc 'yes'
	option country 'ETS'

config features 'features'
	option cbbs_enabled '1'
	option callforward_enabled '1'
	option redial_enabled '1'
	option callreturn_enabled '1'

config sip_service_provider 'sip0'
	option name 'Account 1'
	option cfim_on '*21*'
	option cfim_off '#21#'
	option cfbs_on '*61*'
	option cfbs_off '#61#'
	option call_return '*69'
	option redial '*66'
	option cbbs_key '5'
	option cbbs_maxretry '5'
	option cbbs_retrytime '300'
	option cbbs_waittime '30'
	option domain 'ims.vodafone.pt'
	option codec0 'alaw'
	option codec1 'g729'
	option port '5060'
	option outboundproxy 'stpa.ims.vodafone.pt'
	option outboundproxyport '5060'
	option is_fax '0'
	option enabled '1'
	option host 'ims.vodafone.pt'
	option priority_ulaw '1'
	option priority_alaw '1'
	option priority_g729 '2'
	option priority_g723 '1'
	option priority_g726 '1'
	option target 'direct'
	list call_lines 'BRCM/0'
	option mailbox '-'
	option ptime_g729 '20'
	option ptime_alaw '20'
	option transport 'udp'

config sip_service_provider 'sip1'
	option name 'Account 2'
	option cfim_on '*21*'
