config wifi-device 'wl0'
        option type 'broadcom'
        option channel 'auto'
        option scantimer '15'
        option country 'EU/13'
        option wmm '1'
        option wmm_noack '0'
        option wmm_apsd '0'
        option txpower '100'
        option rateset 'default'
        option frag '2346'
        option rts '2347'
        option dtim_period '1'
        option beacon_int '100'
        option rxchainps '0'
        option rxchainps_qt '10'
        option rxchainps_pps '10'
        option rifs '0'
        option rifs_advert '0'
        option arate 'auto'
        option doth '0'
        option maxassoc '16'
        option band 'a'
        option bandwidth '20'
        option hwmode '11n'

config wifi-iface
        option device 'wl0'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2'
        option gtk_rekey '3600'
        option key '$WPAKEY'
        option macfilter '0'
        option wps_pbc '1'
        option wmf_bss_enable '1'
        option ssid 'DNA-WELHO-$BSSID4'
        option bss_max '16'

config wifi-device 'wl1'
        option type 'broadcom'
        option channel 'auto'
        option scantimer '10'
        option hwmode 'auto'
        option country 'EU/13'
        option wmm '1'
        option wmm_noack '0'
        option wmm_apsd '0'
        option txpower '100'
        option rateset 'default'
        option frag '2346'
        option rts '2347'
        option dtim_period '1'
        option beacon_int '100'
        option rxchainps '0'
        option rxchainps_qt '10'
        option rxchainps_pps '10'
        option band 'b'
        option rifs '0'
        option rifs_advert '0'
        option doth '0'
        option maxassoc '16'
        option bandwidth '20'
        option txantenna '3'
        option rxantenna '3'

config wifi-iface
        option device 'wl1'
        option network 'lan'
        option mode 'ap'
        option encryption 'psk2'
        option key '$WPAKEY'
        option gtk_rekey '3600'
        option macfilter '0'
        option wps_pbc '1'
        option wmf_bss_enable '1'
        option ssid 'DNA-WELHO-$BSSID4'
        option bss_max '16'
