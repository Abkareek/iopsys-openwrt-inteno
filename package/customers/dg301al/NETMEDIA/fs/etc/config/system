
config system
	option conloglevel '7'
	option cronloglevel '8'
	option hostname 'Inteno'
	option log_size '200'
	option zonename 'Europe/Helsinki'
	option timezone 'EET-2EEST,M3.5.0/3,M10.5.0/4'

config rdate
	option interface 'wan'

config timeserver 'ntp'
	option enable_server '1'
	list server 'ntp1.sth.netnod.se'
	list server 'ntp1.gbg.netnod.se'

config 'upgrade'
option 'fw_check_url' 'http://netmediacloud.fi/NAMES/IOP_DG301AL'
option 'fw_path_url' 'http://netmediacloud.fi/Netmedia/DG301AL/IOP3'
option 'fw_find_ext' '.w'
