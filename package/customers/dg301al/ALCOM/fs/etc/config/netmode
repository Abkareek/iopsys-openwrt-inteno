config mode 'setup'
        option dir '/etc/netmodes'
        option detail 'To change network configuration to preconfigured please select configuration mode from dropdown menu and press Change'
        option curmode 'default'

config netmode 'default'
        option desc 'Default - NAT / NAT / NAT / IPTV / IPTV'
        option conf 'default_nat_nat_nat_iptv_iptv'
        option exp 'Default mode is the default configuration'

config netmode 'Config_1'
        option desc '1 - NAT / NAT / NAT / NAT / IPTV'
        option conf 'config1_nat_nat_nat_nat_iptv'
        option exp 'In this mode LAN1, LAN2, LAN3, GBE NAT. WAN port IPTV'

config netmode 'Config_2'
        option desc '2 - NAT / NAT / NAT / NAT / NAT'
        option conf 'config2_nat_nat_nat_nat_nat'
        option exp 'In this mode all ports NAT'
		
config netmode 'Config_3'
        option desc '3 - Bridge / NAT / NAT / IPTV / IPTV'
        option conf 'config3_bridge_nat_nat_iptv_iptv'
        option exp 'In this mode LAN1 bridge. LAN2 and LAN3 NAT. GBE and WAN port IPTV'
		
		
config netmode 'Config_4'
        option desc '4 - Bridge / NAT / NAT / NAT / IPTV'
        option conf 'config4_bridge_nat_nat_nat_iptv'
        option exp 'In this mode LAN1 Bridged. LAN2, LAN3 and GBE NAT. WAN port IPTV'
		
config netmode 'Config_5'
        option desc '5 - Bridge / NAT / NAT / NAT / NAT'
        option conf 'config5_bridge_nat_nat_nat_nat'
        option exp 'In this mode LAN1 Bridge. LAN2, LAN3, GBE and WAN port NAT'


