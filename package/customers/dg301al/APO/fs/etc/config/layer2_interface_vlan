
config vlan_interface
	option name 'silta'
	option ifname 'atm0.902'
	option baseifname 'atm0'
	option vlan8021q '902'
	option vlan8021p '0'
	option bridge '1'

config vlan_interface
	option name 'silta'
	option ifname 'ptm0.902'
	option baseifname 'ptm0'
	option vlan8021q '902'
	option vlan8021p '0'
	option bridge '1'

config vlan_interface
	option name 'silta'
	option ifname 'eth0.902'
	option baseifname 'eth0'
	option vlan8021q '902'
	option vlan8021p '0'
	option bridge '1'

