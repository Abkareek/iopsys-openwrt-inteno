config vlan_interface
	option ifname 'eth0.20'
	option baseifname 'eth0'
	option vlan8021q '20'
	option vlan8021p '0'
	option bridge '1'
	option name 'ETH_vlan20'

config vlan_interface
	option name 'dsl_835_20'
	option ifname 'atm0.20'
	option baseifname 'atm0'
	option vlan8021q '20'
	option vlan8021p '6'
	option bridge '1'

config vlan_interface
