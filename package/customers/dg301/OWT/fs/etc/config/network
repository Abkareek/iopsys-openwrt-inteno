
config interface 'loopback'
        option is_lan '1'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'lan'
        option is_lan '1'
        option type 'bridge'
        option proto 'static'
        option ipaddr '192.168.1.1'
        option netmask '255.255.255.0'
        option ip6assign '60'
        option ifname 'eth3 eth4 '

config interface 'wan'
        option type 'anywan'
        option ifname 'ptm0.1 atm0.1 eth0.1'
        option 'vendorid' '$HARDWAREID-INTENO'
        option proto 'dhcp'
        option hostname 'Inteno_$MAC4'
        option ipv6 '1'

config globals 'globals'
        option ula_prefix 'fd92:78fb:12e5::/48'

config interface 'wan6'
        option proto 'dhcpv6'
        option ifname '@wan'

config interface 'IPTV'
        option type 'bridge'
        option proto 'none'
        option ifname 'eth0.501 eth1 eth2'


