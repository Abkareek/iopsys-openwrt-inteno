config mode 'setup'
        option dir '/etc/netmodes'
        option detail ''
        option curmode 'routed'

config netmode 'routed'
        option desc 'Fully Routed (NAT)'
        option conf 'config_routed'
        option exp 'Default mode. Routed configuration.'

config netmode 'bridged'
        option desc 'Bridged LAN4'
        option conf 'config_bridged'
        option exp 'This mode bridges LAN4 port'

