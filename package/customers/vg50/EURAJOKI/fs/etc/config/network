config interface 'loopback'
        option is_lan '1'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.0.0.0'

config interface 'lan'
        option is_lan '1'
        option type 'bridge'
        option proto 'static'
        option ipaddr '192.168.1.1'
        option netmask '255.255.255.0'
        option ip6assign '60'
        option bridge_instance '1'
        option ifname 'eth0 eth1 eth2 eth3'

config interface 'wan'
        option vendorid 'VG50A-WU7U-INTENO'
        option proto 'dhcp'
        option hostname 'Inteno_$MAC4'
        option ipv6 '1'
        option type 'bridge'
        option bridge_instance '2'
        option ifname 'atm0.1 eth4 ptm0.1'

