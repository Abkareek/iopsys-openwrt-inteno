
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.1.1'
	option netmask '255.255.255.0'
	option mtu '1500'
	option ifname 'eth0 eth1 eth2'

config interface 'Internet'
	option proto 'dhcp'
	option type 'anywan'
	option ifname 'atm0.1 ptm0.408'
	option vendorid 'VG50A-WU7U-INTENO'
	option hostname 'Inteno-VG50'

config interface 'IPTV'
	option type 'bridge'
	option proto 'none'
	option ifname 'atm1.1 eth3 eth4 ptm0.405'
	option auto '1'

