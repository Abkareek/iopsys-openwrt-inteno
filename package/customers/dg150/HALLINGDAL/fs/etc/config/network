
config interface 'loopback'
	option is_lan '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option is_lan '1'
	option type 'bridge'
	option proto 'static'
	option ipaddr '192.168.145.1'
	option netmask '255.255.255.0'
	option ifname 'eth3 eth2 eth1 eth0'
	option ip6assign '60'

config interface 'wan'
	option type 'anywan'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option vendorid 'DG150B-WU7P2U-INTENO'
	option ifname 'eth4.1 atm0.1'
	option ipv6 '1'

config interface 'wan2'
	option proto 'dhcp'
	option hostname 'Inteno_$MAC4'
	option vendorid 'DG150B-WU7P2U-INTENO'
	option ifname 'atm1.1'

config interface 'wan6'
	option proto 'dhcpv6'
	option ifname '@wan'
